+import java.util.*;
+
+class Solution {
+
+    public static Stack<int[]> stack = new Stack<>();
+
+    public static void main(String[] args) {
+        int [][] arr = {{1, 1, 1, 0},{1, 2, 2, 0},{1, 0, 0, 1}, {0, 0, 0, 1}, {0, 0, 0, 3}, {0, 0, 0, 3}};
+        System.out.println(solution(6, 4, arr));
+    }
+
+    public static int[] solution(int m, int n, int[][] picture) {
+        int numberOfArea = 0;
+        int maxSizeOfOneArea = 0;
+
+        for (int i = 0; i < m; i++) {
+            for (int j = 0; j < n; j++) {
+                if (picture[i][j] != 0) {
+                    numberOfArea++;
+                    int count = dfs(i, j, picture);
+                    maxSizeOfOneArea = Math.max(maxSizeOfOneArea, count);
+                }
+            }
+        }
+        int[] answer = new int[2];
+        answer[0] = numberOfArea;
+        answer[1] = maxSizeOfOneArea;
+        return answer;
+    }
+
+    public static int dfs(int x, int y, int[][] graph) {
+        int count = 0;
+        int color = graph[x][y];
+        int[] curr = {x, y};
+        stack.add(curr);
+
+        int[] dx = {1, -1, 0, 0};
+        int[] dy = {0, 0, 1, -1};
+
+        while (!stack.isEmpty()) {
+            curr = stack.pop();
+            for (int i = 0; i < 4; i++) {
+                int nx = curr[0] + dx[i];
+                int ny = curr[1] + dy[i];
+                if (nx >=0 && nx < graph.length && ny >= 0 && ny < graph[0].length && graph[nx][ny] == color) {
+                    graph[nx][ny] = 0;
+                    count++;
+                    int[] temp = {nx, ny};
+                    stack.add(temp);
+                }
+            }
+        }
+        return count;
+    }
+}
